                    parameter[1] = False # –ø—Ä–∏—Å–≤–æ–µ–º –∑–Ω–∞—á–µ–Ω–∏–µ "False"

                elif parameter[1].find(";") != -1: # –µ—Å–ª–∏ –µ—Å—Ç—å –ø–∞—Ä–∞–º–µ—Ç—Ä —Å ";", –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –µ–≥–æ
